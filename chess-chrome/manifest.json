{
  "manifest_version": 2,

  "name": "Chess Chrome",
  "description": "Scrapes the chess.com/livechess webpage.",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["http://live.chess.com/*"],
      "js": ["jquery.js", "parser.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "<all_urls>"
  ]

}